find_outliers <- function(x,col){
q1 <- quantile(x,0.25)
q3 <- quantile(x,0.75)
iqr <- q3 - q1
lower <- q1 - 1.5 * iqr
upper <- q3 + 1.5 * iqr
outliers <- x[x < lower | x> upper]
return(outliers)
}
